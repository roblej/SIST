<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="dept">
    <select id="getDept" resultType="am.vo.DeptVO" parameterType="String">
        select * from dept
        where deptno = #{key}
    </select>
</mapper>

